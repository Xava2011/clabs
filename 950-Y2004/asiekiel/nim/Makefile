####### Build rules

all: Makefile $(TARGET)

$(TARGET):  $(UICDECLS) $(OBJECTS) $(OBJMOC) 
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(LIBS)

mocables: $(SRCMOC)

$(MOC): 
	( cd $(QTDIR)/src/moc ; $(MAKE) )

Makefile: kik.pro  $(QTDIR)/share/qt/mkspecs/default/qmake.conf 
	$(QMAKE) -o Makefile kik.pro
qmake: 
	@$(QMAKE) -o Makefile kik.pro

dist: 
	@mkdir -p .tmp/kik && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .tmp/kik/ && ( cd `dirname .tmp/kik` && $(TAR) kik.tar kik && $(GZIP) kik.tar ) && $(MOVE) `dirname .tmp/kik`/kik.tar.gz . && $(DEL_FILE) -r .tmp/kik

